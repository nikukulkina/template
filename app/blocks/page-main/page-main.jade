include /blocks/container/container
include /blocks/header/header
include /blocks/main/main
include /blocks/form/form
include /blocks/footer/footer
include /blocks/copyrights/copyrights

mixin page-main()
	+b.page-main&attributes(attributes)
		+e.header-wrap
			+container()
				+header()
		+main()
		+e.form-wrap
			+container()
				+form()
		+e.footer-wrap
			+container()
				+footer()
		+e.copyrights-wrap
			+container()
				+copyrights()
